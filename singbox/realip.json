{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:1234",
      "secret": ""
    },
    "cache_file": {
      "enabled": true,
      "store_fakeip": false
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "dns_local",
        "type": "local"
      },
      {
        "tag": "dns_proxy",
        "type": "https",
        "server": "dns.google",
        "detour": "Proxy",
        "domain_resolver": "dns_direct"
      },
      {
        "tag": "dns_direct",
        "type": "https",
        "server": "dns.alidns.com",
        "domain_resolver": "dns_local"
      }
    ],
    "rules": [
      {
        "clash_mode": "Direct",
        "server": "dns_direct"
      },
      {
        "clash_mode": "Global",
        "server": "dns_proxy"
      },
      {
        "query_type": "HTTPS",
        "action": "reject"
      },
      {
        "rule_set": [
          "private_domain"
        ],
        "server": "dns_local"
      },
      {
        "rule_set": [
          "google_domain",
          "github_domain",
          "steam_domain",
          "proxy_domain"
        ],
        "server": "dns_proxy"
      },
      {
        "rule_set": [
          "cn_domain"
        ],
        "server": "dns_direct"
      }
    ],
    "independent_cache": true,
    "final": "dns_proxy",
    "strategy": "ipv4_only"
  },
  "inbounds": [
    {
      "type": "tun",
      "address": [
        "172.19.0.1/30"
      ],
      "auto_route": true,
      "strict_route": true,
      "stack": "mixed",
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 7890
        }
      }
    },
    {
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 7890
    }
  ],
  "outbounds": [
    {
      "type": "selector",
      "tag": "Proxy",
      "interrupt_exist_connections": true,
      "outbounds": []
    },
    {
      "type": "direct",
      "tag": "Direct"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "default_domain_resolver": {
      "server": "dns_direct"
    },
    "final": "Proxy",
    "rules": [
      {
        "action": "sniff",
        "timeout": "500ms",
        "sniffer": [
          "http",
          "tls",
          "dns"
        ]
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 53
          },
          {
            "protocol": "dns"
          }
        ],
        "action": "hijack-dns"
      },
      {
        "rule_set": [
          "private_domain"
        ],
        "outbound": "Direct"
      },
      {
        "ip_is_private": true,
        "outbound": "Direct"
      },
      {
        "clash_mode": "Global",
        "outbound": "Proxy"
      },
      {
        "clash_mode": "Direct",
        "outbound": "Direct"
      },
      {
        "action": "reject",
        "rule_set": "reject_domain"
      },
      {
        "rule_set": [
          "telegram_ip",
          "google_domain",
          "google_ip",
          "github_domain",
          "steam_domain",
          "proxy_domain"
        ],
        "outbound": "Proxy"
      },
      {
        "rule_set": [
          "cn_domain",
          "cn_ip"
        ],
        "outbound": "Direct"
      }
    ],
    "rule_set": [
      {
        "tag": "private_domain",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/private.srs",
        "format": "binary",
        "download_detour": "Direct"
      },
      {
        "tag": "reject_domain",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/category-ads-all.srs",
        "format": "binary",
        "download_detour": "Direct"
      },
      {
        "tag": "cn_ip",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geoip/cn.srs",
        "format": "binary",
        "download_detour": "Direct"
      },
      {
        "tag": "cn_domain",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/cn.srs",
        "format": "binary",
        "download_detour": "Direct"
      },
      {
        "tag": "google_ip",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geoip/google.srs",
        "format": "binary",
        "download_detour": "Direct"
      },
      {
        "tag": "google_domain",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/google.srs",
        "format": "binary",
        "download_detour": "Direct"
      },
      {
        "tag": "telegram_ip",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geoip/telegram.srs",
        "format": "binary",
        "download_detour": "Direct"
      },
      {
        "tag": "github_domain",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/github.srs",
        "format": "binary",
        "download_detour": "Direct"
      },
      {
        "tag": "steam_domain",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/steam.srs",
        "format": "binary",
        "download_detour": "Direct"
      },
      {
        "tag": "proxy_domain",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/geolocation-!cn.srs",
        "format": "binary",
        "download_detour": "Direct"
      }
    ]
  }
}
